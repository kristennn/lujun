<h2>Product List</h2>
<%= link_to("新增商品", new_admin_product_path, class: "btn btn-primary pull-right") %>
<table class="table table-border">
  <thead>
    <th>#</th>
    <th>Product Pic</th>
    <th>Name</th>
    <th>Price</th>
    <th>Options</th>
  </thead>
  <tbody>
    <% @products.each do |product| %>
      <td><%= product.id %></td>
      <td><% %></td>
      <td><%= product.title %></td>
      <td><%= product.price %></td>
      <td>
        <%= link_to("Edit", edit_product_path(product)) %>
        <%= link_to("Delete", product_path(product), method: :delete) %>
      </td>
    <% end %>
  </tbody>
</table>
